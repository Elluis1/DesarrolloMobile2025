{
  "kind": "collectionType",
  "collectionName": "marcas",
  "info": {
    "singularName": "marca",
    "pluralName": "marcas",
    "displayName": "Marca"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
